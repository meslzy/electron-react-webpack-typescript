{
  "name": "electron-react-webpack-typescript",
  "version": "1.0.0",
  "description": "the best structure for getting started with electron react webpack and typescript",
  "author": {
    "name": "meslzy",
    "email": "i@meslzy.com"
  },
  "main": "dist/main.js",
  "scripts": {
    "main:start": "webpack --config config/webpack.main.js --env dev && electron .",
    "main:build": "webpack --config config/webpack.main.js",
    "render:start": "webpack-dev-server --config config/webpack.render.js --env dev",
    "render:build": "webpack --config config/webpack.render.js",
    "prepack": "npm run main:build && npm run render:build",
    "pack": "node config/electron.builder.js"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@types/node": "^14.14.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-env": "^1.15.3",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "file-loader": "^6.2.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^5.0.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "license": "ISC"
}
